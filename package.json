{
  "name": "meteor-mantra-kickstarter",
  "version": "0.2.1",
  "scripts": {
    "lint": "npm run lint_pkgs && npm run lint_main",
    "lint_main": "eslint . --ext .js --ext .jsx",
    "lint_pkgs": "./.pkgs/lint_each_package.sh",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha .scripts/mocha_boot.js client/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js,jsx",
    "acceptance": "./node_modules/.bin/chimp .e2e_tests/chimp-config.js --ddp=http://localhost:3000         --path=.e2e_tests",
    "e2e": "./node_modules/.bin/chimp .e2e_tests/chimp-config.js --ddp=http://localhost:3000 --watch --path=.e2e_tests",
    "version-check": "ncu",
    "qtst": "mocha .scripts/mocha_boot.js client/access_control/tests/**/*.js --compilers js:babel-core/register",
    "qtw": "npm run qtst -- --watch --watch-extensions js,jsx",
    "dsstore": "find . -name '*.DS_Store' -type f -delete",
    "knex_prod": "export T=production; npm run knex_roll && npm run knex_make && npm run knex_seed;",
    "knex_cont": "export T=continuousIntegration; npm run knex_roll && npm run knex_make && npm run knex_seed;",
    "knex_roll": "cd server/api/.knex && knex migrate:rollback --env ${T};",
    "knex_make": "cd server/api/.knex && knex migrate:latest --env ${T};",
    "knex_seed": "cd server/api/.knex && knex seed:run --env ${T};",
    "runp": "./.scripts/free.sh; export HOST_SERVER_NAME=\"http://moon.planet.sun:3000/\"; export ROOT_URL=\"${HOST_SERVER_NAME}\"; export YOUR_FULLNAME=\"You Yourself\"; export GITHUB_ORGANIZATION_NAME=\"YourOrg\"; meteor run --mobile-server=${HOST_SERVER_NAME} --production --verbose --no-lint --no-release-check --settings=settings.json;",
    "rund": "./.scripts/free.sh; export HOST_SERVER_NAME=\"http://moon.planet.sun:3000/\"; export ROOT_URL=\"${HOST_SERVER_NAME}\"; export YOUR_FULLNAME=\"You Yourself\"; export GITHUB_ORGANIZATION_NAME=\"YourOrg\"; meteor run --mobile-server=${HOST_SERVER_NAME}              --verbose --no-lint --no-release-check --settings=settings.json;"
  },
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-plugin-react-require": "3.0.0",
    "babel-polyfill": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "chai": "3.5.0",
    "chimp": "0.47.0",
    "enzyme": "2.7.0",
    "eslint": "3.14.0",
    "eslint-plugin-babel": "4.0.1",
    "eslint-plugin-react": "6.9.0",
    "jsdom": "9.9.1",
    "mocha": "3.2.0",
    "react-addons-test-utils": "15.4.2",
    "sinon": "1.17.7"
  },
  "dependencies": {
    "apollo-client": "^0.5.0",
    "babel-runtime": "6.22.0",
    "bcrypt": "1.0.2",
    "body-parser": "^1.16.0",
    "classnames": "2.2.5",
    "domready": "1.0.8",
    "express": "^4.14.0",
    "formsy-react": "0.19.2",
    "formsy-react-components": "0.10.1",
    "graphql": "^0.8.0",
    "graphql-server-express": "^0.4.3",
    "graphql-tag": "^1.2.3",
    "graphql-tools": "^0.8.0",
    "knex": "^0.12.6",
    "lodash": "4.17.4",
    "logatim": "0.9.0",
    "loggly": "1.1.1",
    "mantra-core": "1.7.0",
    "meteor-node-stubs": "0.2.5",
    "mkdirp": "0.5.1",
    "mmks_book": "0.0.1",
    "mmks_widget": "0.0.1",
    "mysql": "^2.13.0",
    "nodemailer": "2.7.2",
    "nodemailer-mailgun-transport": "1.2.4",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "pg-native": "^1.10.0",
    "react": "15.4.2",
    "react-apollo": "^0.9.0",
    "react-dom": "15.4.2",
    "react-mounter": "1.2.0",
    "react-select": "1.0.0-rc.2",
    "react-simple-di": "1.2.0",
    "react-tagsinput": "3.14.0",
    "sequelize": "^3.30.0",
    "simpl-schema": "^0.1.0",
    "sqlite3": "^3.1.8",
    "tcomb-form": "0.9.10",
    "touch": "1.0.0",
    "uniforms": "^1.9.0",
    "uniforms-bootstrap3": "^1.9.0",
    "winston": "2.3.1",
    "winston-loggly-bulk": "1.3.4"
  },
  "private": true
}
